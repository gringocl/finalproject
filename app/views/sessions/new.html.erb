<div class='small-6 large-centered columns'><h1>Sign in</h1>
<%= form_for :signin, method: "POST" do |f| %>
<div class="panel "><p>
  <%= f.label :email %>
  <%= f.text_field :email %>
</p>

<p>
  <%= f.label :password %>
  <%= f.password_field :password %>
</p>


<%= f.submit "Sign in", class: 'button radius' %>
<% end %></div></div>